{
  "manifest_version": 2,
  "name": "GroupMe Enhanced",
  "description": "Provides sound and desktop notifications for GroupMe messages",
  "version": "0.1.0",
  "icons": {
    "16": "img/icon16.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://app.groupme.com/chats"
      ],
      "js": [
        "js/notifications.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "js/content-script.js",
    "audio/*"
  ],
  "permissions": [
  ]
}
